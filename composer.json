{
    "name": "thepublicgood/pcflib",
    "description": "Generate the XML feed for pricecheck.co.za price comparrison service.",
    "license": "MIT",
    "authors": [
        {
            "name": "warrick",
            "email": "me@warrickbayman.co.za"
        }
    ],
    "require": {
        "php": "7.1.8",
        "ext-json": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "TPG\\Pcflib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
